# GraphQL schema example
#
# https://gqlgen.com/getting-started/

#Category GraphQL

type Category {
  id: ID!
  name: String!
  description: String!
}

type Query {
  category(id: ID!): Category
   hello: String!
}

type Mutation {
  createCategory(name: String!, description: String!): Category
  setMessage(message: String!): String!
}

